{
  "branches": [
    "main",
    {
      "name": "beta",
      "prerelease": true
    },
    {
      "name": "alpha",
      "prerelease": true
    }
  ],
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    "@semantic-release/changelog",
    [
      "@semantic-release/npm",
      {
        "npmPublish": true,
        "tarballDir": "dist",
        "pkgRoot": "."
      }
    ],
    [
      "@semantic-release/git",
      {
        "assets": [
          "package.json",
          "package-lock.json",
          "CHANGELOG.md"
        ],
        "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
      }
    ],
    "@semantic-release/github"
  ],
  "preset": "conventionalcommits",
  "releaseRules": [
    {
      "type": "docs",
      "scope": "README",
      "release": "patch"
    },
    {
      "type": "refactor",
      "release": "patch"
    },
    {
      "type": "style",
      "release": false
    },
    {
      "scope": "no-release",
      "release": false
    }
  ],
  "parserOpts": {
    "noteKeywords": [
      "BREAKING CHANGE",
      "BREAKING CHANGES",
      "BREAKING"
    ]
  }
}